{
  "name": "@pubcore/vite-storybook-mui",
  "version": "0.3.0",
  "description": "Storybook for react components based on MUI",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "serve": "vite preview",
    "prepare": "patch-package && husky install && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "tsc --noEmit && eslint ."
  },
  "peerDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/material": "^5.1.1",
    "i18next": "^21.5.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^7.17.4",
    "react-i18next": "^11.14.3",
    "react-router-dom": "^6.0.1"
  },
  "dependencies": {
    "immutability-helper": "^3.1.1",
    "lodash-es": "^4.17.21",
    "react-dropzone": "^11.4.2",
    "react-virtualized": "^9.22.3",
    "reselect": "^4.1.5",
    "string-similarity": "^4.0.4",
    "throat": "^6.0.1",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-virtualized": "^9.21.14",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^1.0.5",
    "axios": "^0.24.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^12.1.2",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.1",
    "storybook-dark-mode": "^1.0.9-canary.166.3604.0",
    "typescript": "^4.3.2",
    "vite": "^2.7.13",
    "webpack": "^5.64.4"
  },
  "keywords": [
    "MUI",
    "storybook"
  ],
  "author": "pubcore",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:pubcore/vite-storybook-mui.git"
  }
}
